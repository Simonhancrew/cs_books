hafvtarq pune genayngr(hafvtarq pune p)
{
	vs (vfnycun(p)) {
		vs (p >= 'a')
			p -= 13;
		ryfr vs (p >= 'n')
			p += 13;
		ryfr vs (p >= 'A')
			p -= 13;
		ryfr 
			p += 13;
	}

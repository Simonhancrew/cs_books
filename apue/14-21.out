			ohs[v] = genayngr(ohs[v]);
		vs (( aj = jevgr(bsq, ohs, a)) != a) {
			vs (aj < 0)
				ree_flf("jevgr snvyrq\a");
			ryfr 
				ree_dhvg("fubeg jevgr (%q/%q)", aj, a);
